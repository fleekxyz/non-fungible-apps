generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model builds {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  commitHash       String
  domain           String
  githubRepository String
  ipfsHash         String
}

model tokens {
  id               String  @id @default(auto()) @map("_id") @db.ObjectId
  commitHash       String
  domain           String
  githubRepository String
  ipfsHash         String
  owner            String
  tokenId          Int
  verified         Boolean
}
