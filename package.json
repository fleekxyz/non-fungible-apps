    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
